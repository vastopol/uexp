getarg.c.15	;* wlbl(x)	(x) bytes beyone wlbl
	
	
	txt break('(') . disp '(' len(2) . indx	
	lit = getrip(disp)
	op(mov.w,reg.w0,lit)
	op(add.w,reg.w0,regmap[indx])
	getarg = '(' reg.w0 ')'
						:(return)

getarg.c.16	;* integer
	getarg = txt			:(getarg.done)

*	signed real

getarg.c.17 
	getarg = txt			:(getarg.done)

getarg.c.18	;* =dlbl
	getarg = iv(substr(txt,2))		:(getarg.done)

getarg.c.19	;* *dlbl
	getarg = iv(cfp_b * v(substr(txt,2)))	:(getarg.done)

getarg.c.20	;* =name (data section)
getarg.c.21	;* =clbl (constant section)
getarg.c.22	;* =elbl
	getarg = getrip(substr(txt,2))		:(getarg.done)

*	pnam, eqop
getarg.c.23
getarg.c.24 
	getarg = txt				:(getarg.done)

*	ptyp, text, dtext
getarg.c.25
getarg.c.26
getarg.c.27 getarg = txt			:(getarg.done)
getarg.done
