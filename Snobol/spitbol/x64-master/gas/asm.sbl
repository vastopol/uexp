*	imem is null to generate memory reference, otherwise just get address for use in 'Lea_' instruction.

	txt = i.text(iarg)
	typ = i.type(iarg)
	i_suspend = 1
	eq(typ)					:f($(argcase[typ]))
	getarg = txt				:(getarg.done)

getarg.c.1 	;* int		integer
	getarg = txt				:(getarg.done)

getarg.c.2 	;* dlbl		symbol defined in definitions section
	getarg = txt				:(getarg.done)


getarg.c.3	;* wlbl		label in working section
getarg.c.4 	;* clcl		label in constant section
	getarg = rip(txt)
