getarg.c.5	;* elbl		program section entry label
getarg.c.6 	;* plbl		program section entry (non entry)
	getarg = txt			:(getarg.done)

getarg.c.7	;*		x register
getarg.c.8	;*		w register
	getarg = regmap[txt]			:(getarg.done)


getarg.c.9	;* (x)		 location referenced by x
	txt len(1) len(2) . indx		
	getarg = '(' regmap[indx] ')'
						:(getarg.done)

*	(x)+, register indirect, post increment use lea reg,[reg+cfp_b] unless reg is esp, 
*	since it takes an extra byte.  actually, lea reg,[reg+cfp_b] and add reg,cfp_b are both 
*	2 cycles and 3 bytes for all the other regs, and either could be used.

getarg.c.10	;* (x)+		like (x) but predecrement x
	indx = substr(txt,2,2)
	getarg = '(' regmap[indx] ')'
	genaop(newstmt(,'addq',regmap[indx],literal_ cfp_b)) 
						:(getarg.done)

getarg.c.11	;* -(x)		like (x) but post increment x
	indx = substr(txt,3,2)
	genbop(newstmt(,'subq',regmap[indx],literal_ cfp_b,)) 	
	getarg = '(' regmap[indx] ')'
						:(getarg.done)

