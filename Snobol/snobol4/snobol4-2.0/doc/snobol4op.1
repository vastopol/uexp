%	vertical concat	LEFT	w/ BLOCKS
sp	concatenation	LEFT
@	definable	LEFT
-+	subtract, add	LEFT
#	definable	LEFT	w/o BLOCKS
*	multiplication	LEFT
%	definable	LEFT	w/o BLOCKS
?	definable	LEFT	w/o SPITBOL
/	division	LEFT
^	exponentiation	RIGHT
