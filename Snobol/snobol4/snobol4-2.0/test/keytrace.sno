* tests all types of keyword trace, including STNO/BREAKPOINT
	&STLIMIT = &TRACE = &ERRLIMIT = 10000
	TRACE("STNO", "KEYWORD")
	TRACE("STCOUNT", "KEYWORD")
	TRACE("FNCLEVEL", "KEYWORD")
	TRACE("STFCOUNT", "KEYWORD")
	TRACE("ERRTYPE", "KEYWORD")
	DEFINE("A()")
	BREAKPOINT(15, 1)
	BREAKPOINT(17, 1)
	BREAKPOINT(19, 1)
	BREAKPOINT(21, 1)
	LOOPS = 0
LOOP	OUTPUT = "LOOPS = " LOOPS " ****"
	X = 1 / 0
	A()
	A()
	A()
	A()
	A()
	A()
	A()
	A()
	A()
	X = LEN(-1)
	LOOPS = LOOPS + 1
	BREAKPOINT(13, 0)
	BREAKPOINT(17, 0)
	EQ(LOOPS, 1)	:S(LOOP)
	STOPTR("STCOUNT", "KEYWORD")
	STOPTR("FNCLEVEL", "KEYWORD")
	EQ(LOOPS,2)	:S(LOOP)
	TRACE("STCOUNT", "KEYWORD")
	EQ(LOOPS,3)	:S(LOOP)
	:(END)

A	OUTPUT = (N = N + 1) :(RETURN)
END
