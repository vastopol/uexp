-INCLUDE 'time.sno'

	DEFINE("TMDUMP(T)")	:(E.TMDUMP)
TMDUMP	OUTPUT = "TM_SEC " TM_SEC(T)
	OUTPUT = "TM_MIN " TM_MIN(T)
	OUTPUT = "TM_HOUR " TM_HOUR(T)
	OUTPUT = "TM_MDAY " TM_MDAY(T)
	OUTPUT = "TM_MON " TM_MON(T)
	OUTPUT = "TM_YEAR " TM_YEAR(T)
	OUTPUT = "TM_WDAY " TM_WDAY(T)
	OUTPUT = "TM_YDAY " TM_YDAY(T)
*	OUTPUT = "TM_ISDST " TM_ISDST(T)
*	OUTPUT = "TM_GMTOFF " TM_GMTOFF(T)
	:(RETURN)
E.TMDUMP

* 2010-12-17 03:17:32 GMT
	T = 1292555852

	TV = GETTIMEOFDAY()
	OUTPUT = LE(TV_SEC(TV),T) "TV_SEC " TV_SEC(TV)

	GTM = GMTIME(T)
	OUTPUT = "gmtime:"
	TMDUMP(GTM)

	OUTPUT =
	OUTPUT = STRFTIME("%Y-%m-%d %H:%M:%S", GTM)

	OUTPUT =
	OUTPUT = MKTIME(LOCALTIME(T))
END
